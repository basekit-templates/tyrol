{% extends "layout.twig" %}

{% block content %}

<div class="row container">
    <div class="nine columns push">
        <header>
            {{widget('companyname', 'tyrol', {"business": "TYROL"})|raw}}
            {{widget('clicktocall', 'tyrol-call', {phoneText: '0800 123 123'})|raw}}
        </header>  
        <div class="main clearfix">
            {{zone('main')|raw}}
        </div>  
    </div>
    <div class="three columns pull">
        <footer>
            {{widget('extendednavigation', 'navigationheader', {})|raw}}
        </footer>
    </div>
</div>

<footer>
    <div class="wrapper clearfix">
    {{widget('content', 'footercolumn1content', {"content":"<p>Lorem Ipsum Dolor</p>"})|raw}}
    </div>
        {{widget('profile', 'footercolumn1socialicons', { 'profileType':'socialicons', 'socialicons':'profile'})|raw}}
</footer>

{% endblock %}
